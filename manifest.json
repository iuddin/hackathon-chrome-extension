
{

  "manifest_version": 2,
  "name": "Time Out",
  "version": "1.0",
  "description": "A nifty little productivity tool",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["./main.js"]
    }
  ],
  "icons": {
    "16": "Time-PNG-Image.png",
    "48": "Time-PNG-Image.png",
    "128": "Time-PNG-Image.png" 
  },
  "web_accessible_resources": [ "*.mp3"],
  "options_page": "index.html",  
  "permissions": [
    "storage"
  ]
  

}